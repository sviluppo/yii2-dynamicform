{
    "name": "sviluppo/yii2-dynamicform",
    "description": "It is widget to yii2 framework to clone form elements in a nested manner, maintaining accessibility.",
    "keywords": [
        "sviluppo",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/sviluppo/yii2-dynamicform/issues",
        "wiki": "https://github.com/sviluppo/yii2-dynamicform/wiki/",
        "source": "https://github.com/sviluppo/yii2-dynamicform"
    },
    "authors": [
        {
            "name": "Alberto Tavoletti",
            "email": "alberto@pixelsc.it",
            "homepage": "http://sviluppo.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "symfony/css-selector": "^4.1",
        "symfony/dom-crawler": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "sviluppo\\dynamicform\\": "src"
        }
    }
}
